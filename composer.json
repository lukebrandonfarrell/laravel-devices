{
    "name": "lukebrandonfarrell/laravel-devices",
    "description": "Adds device infrastructure for storing connected devices and authenticating anonymous users to your application",
    "keywords": ["laravel", "devices", "platforms", "tracking", "authentication", "mobile", "web"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "lukebrandonfarrell",
            "email": "lukebrandonfarrell@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
      "php": "^7.0",
      "laravel/framework": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "lukebrandonfarrell\\laravel-devices\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "lukebrandonfarrell\\laravel-devices\\src\\DeviceServiceProvider"
            ]
        }
    }
}
